# Convex backend environment example
# Copy this file to `.env.local` and fill in the values you need.

NODE_ENV=development

# Convex deployment URLs
CONVEX_SITE_URL=http://localhost:3001
CONVEX_CLOUD_URL=
NEXT_PUBLIC_CONVEX_URL=

# Shopify credentials
SHOPIFY_API_KEY=
SHOPIFY_API_SECRET=
SHOPIFY_API_VERSION=2025-07

# Resend / Auth
AUTH_RESEND_KEY=

# Firecrawl configuration
FIRECRAWL_API_KEY=

# Meta integrations
META_APP_ID=
META_APP_SECRET=
META_API_VERSION=v23.0
META_BASE_URL=https://graph.facebook.com
META_TICK_MINUTES=15
META_BATCH_SIZE=15
LOG_META=0

# Webhook logging
LOG_WEBHOOKS=0
